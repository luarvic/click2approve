{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch WebApi",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build-webapi",
      "program": "${workspaceFolder}/api/src/Click2Approve.WebApi/bin/Debug/net10.0/Click2Approve.WebApi.dll",
      "args": [],
      "cwd": "${workspaceFolder}/api/src/Click2Approve.WebApi",
      "stopAtEntry": false,
      "console": "integratedTerminal",
      "env": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      }
    },
    {
      "name": "Attach to WebApi",
      "type": "coreclr",
      "request": "attach"
    }
  ]
}
